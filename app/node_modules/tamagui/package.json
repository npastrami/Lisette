{
  "name": "tamagui",
  "version": "1.79.12",
  "sideEffects": [
    "*.css",
    "setup.js"
  ],
  "source": "src/index.ts",
  "types": "./types/index.d.ts",
  "main": "dist/cjs",
  "module": "dist/esm",
  "module:jsx": "dist/jsx",
  "tamagui": {
    "bundle.native": "./src/index.ts",
    "bundle.native.test": "./src/index.ts"
  },
  "files": [
    "src",
    "types",
    "dist",
    "linear-gradient",
    "native.js",
    "native.d.ts"
  ],
  "scripts": {
    "build": "tamagui-build",
    "watch": "tamagui-build --watch",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build",
    "lint": "../../node_modules/.bin/biome check src",
    "lint:fix": "../../node_modules/.bin/biome check --apply-unsafe src",
    "check": "yarn check-circular-deps",
    "check-circular-deps": "npx madge --circular ./src/index.ts"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./native": {
      "types": "./types/index.d.ts",
      "import": "./dist/native.js",
      "require": "./dist/native.js"
    },
    "./native-test": {
      "types": "./types/index.d.ts",
      "import": "./dist/test.native.js",
      "require": "./dist/test.native.js"
    },
    "./linear-gradient": {
      "types": "./types/linear-gradient.d.ts",
      "import": "./dist/esm/linear-gradient.js",
      "require": "./dist/cjs/linear-gradient.js"
    }
  },
  "dependencies": {
    "@tamagui/accordion": "1.79.12",
    "@tamagui/adapt": "1.79.12",
    "@tamagui/alert-dialog": "1.79.12",
    "@tamagui/animate-presence": "1.79.12",
    "@tamagui/avatar": "1.79.12",
    "@tamagui/button": "1.79.12",
    "@tamagui/card": "1.79.12",
    "@tamagui/checkbox": "1.79.12",
    "@tamagui/compose-refs": "1.79.12",
    "@tamagui/core": "1.79.12",
    "@tamagui/create-context": "1.79.12",
    "@tamagui/dialog": "1.79.12",
    "@tamagui/fake-react-native": "1.79.12",
    "@tamagui/focusable": "1.79.12",
    "@tamagui/font-size": "1.79.12",
    "@tamagui/form": "1.79.12",
    "@tamagui/get-button-sized": "1.79.12",
    "@tamagui/get-font-sized": "1.79.12",
    "@tamagui/get-token": "1.79.12",
    "@tamagui/helpers": "1.79.12",
    "@tamagui/helpers-tamagui": "1.79.12",
    "@tamagui/image": "1.79.12",
    "@tamagui/label": "1.79.12",
    "@tamagui/linear-gradient": "1.79.12",
    "@tamagui/list-item": "1.79.12",
    "@tamagui/polyfill-dev": "1.79.12",
    "@tamagui/popover": "1.79.12",
    "@tamagui/popper": "1.79.12",
    "@tamagui/portal": "1.79.12",
    "@tamagui/progress": "1.79.12",
    "@tamagui/radio-group": "1.79.12",
    "@tamagui/react-native-media-driver": "1.79.12",
    "@tamagui/scroll-view": "1.79.12",
    "@tamagui/select": "1.79.12",
    "@tamagui/separator": "1.79.12",
    "@tamagui/shapes": "1.79.12",
    "@tamagui/sheet": "1.79.12",
    "@tamagui/slider": "1.79.12",
    "@tamagui/stacks": "1.79.12",
    "@tamagui/switch": "1.79.12",
    "@tamagui/tabs": "1.79.12",
    "@tamagui/text": "1.79.12",
    "@tamagui/theme": "1.79.12",
    "@tamagui/toggle-group": "1.79.12",
    "@tamagui/tooltip": "1.79.12",
    "@tamagui/use-controllable-state": "1.79.12",
    "@tamagui/use-debounce": "1.79.12",
    "@tamagui/use-force-update": "1.79.12",
    "@tamagui/use-window-dimensions": "1.79.12",
    "@tamagui/visually-hidden": "1.79.12",
    "reforest": "^0.13.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native-web": "*"
  },
  "devDependencies": {
    "@tamagui/build": "1.79.12",
    "react": "^18.2.0",
    "react-native-web": "^0.19.9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tamagui/tamagui.git",
    "directory": "packages/tamagui"
  },
  "gitHead": "a49cc7ea6b93ba384e77a4880ae48ac4a5635c14"
}
